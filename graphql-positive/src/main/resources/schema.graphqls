schema {
	query : QueryType
}

type QueryType {
	artist (name : String) : Character
	album : Album
	track : Track
} 

type Character {
	id   : Long
	name : String
	imageUrl : String
}

type Album {
	name : String
	imageUrl : String
	track : [Track]
}

type Track {
	name : String
	preview_url : String
	track_number : Int
}