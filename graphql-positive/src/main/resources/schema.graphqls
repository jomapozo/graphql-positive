schema {
	query : QueryType
}

type QueryType {
	artist (name : String) : Artista
} 

type Artista {
	id   : Long
	name : String
	imageUrl : String
	albums : [Album]
}

type Album {
	name : String
	imageUrl : String
	tracks : [Track]
}

type Track {
	name : String
	previewUrl : String
	trackNumber : Int
	artist : [Artista]
}